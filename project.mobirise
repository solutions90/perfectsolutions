globalexpo.globalgroup2000.com/#:~:text=%D8%AA%D9%82%D9%88%D9%85%20%D8%AC%D9%84%D9%88%D8%A8%D8%A7%D9%84%20%D8%A7%D9%83%D8%B3%D8%A8%D9%88%20%D8%A8%D8%AA%D9%88%D9%81%D9%8A%D8%B1%20%D8%A7%D9%84%D8%AE%D8%AF%D9%85%D8%A7%D8%AA,%D8%A7%D9%84%D8%AA%D9%8A%20%D8%B3%D9%8A%D8%AA%D9%85%20%D9%81%D9%8A%D9%87%D8%A7%20%D8%A7%D9%84%D8%AA%D8%AF%D8%B1%D9%8A%D8%A8%20%D8%A8%D8%A7%D9%84%D9%81%D9%86%D8%AF%D9%82%D8%8C\"><img src=\"@PROJECT_PATH@/assets/images/expo-ny7q4unjzrfjqoswob5t9l5ehnadks1mxy4dlwn682-185x77.jpg\" alt=\"Mobirise Website Builder\"></a>\n            </div>\n            <div class=\"col-md-3 card\" mbr-if=\"items > 1\">\n                <a href=\"https://www.pepsicojobs.com/egypt/jobs\"><img src=\"@PROJECT_PATH@/assets/images/pepsico20egypt-240x320.jpg\" alt=\"Mobirise Website Builder\"></a>\n            </div>\n            <div class=\"col-md-3 card\" mbr-if=\"items > 2\">\n                <a href=\"https://www.mansourgroup.com/\"><img src=\"@PROJECT_PATH@/assets/images/mansour-group-logo.svg\" alt=\"Mobirise Website Builder\"></a>\n            </div>\n            <div class=\"col-md-3 card\" mbr-if=\"items > 3\">\n                <a href=\"https://www.elsewedyelectric.com/?srsltid=AfmBOoqBgauZVZGozIvFIa1a9ztheHsdxR0oka4YiypwZv_K9JSMDEek\"><img src=\"@PROJECT_PATH@/assets/images/image001-240x74.png\" alt=\"Mobirise Website Builder\"></a>\n            </div>\n            <div class=\"col-md-3 card\" mbr-if=\"items > 4\">\n                <a href=\"https://www.a2zadvertising.com/\"><img src=\"@PROJECT_PATH@/assets/images/pic-240x151.png\" alt=\"Mobirise Website Builder\"></a>\n            </div>\n            <div class=\"col-md-3 card\" mbr-if=\"items > 5\">\n                <img src=\"@PROJECT_PATH@/assets/images/images-225x225.png\" alt=\"Mobirise Website Builder\">\n            </div>\n            <div class=\"col-md-3 card\" mbr-if=\"items > 6\">\n                <a href=\"https://foxadv.com/\"><img src=\"@PROJECT_PATH@/assets/images/fox-advertising-agency-logo20copy-200x200.jpg\" alt=\"Mobirise Website Builder\"></a>\n            </div>\n            <div class=\"col-md-3 card\" mbr-if=\"items > 7\">\n                <a href=\"https://meacoms.net/\"><img src=\"@PROJECT_PATH@/assets/images/logo-1760a5d783924ea9aed4c55d7e928087-240x144.jpg\" alt=\"Mobirise Website Builder\"></a>\n            </div>\n        </div>\n    </div>\n</section>",
